
//Here we are considering total Days in a month is 31
var k = 1;
vsr totalWeeksInAMonth = 6;
for(var i=1 ; i<=totalWeeksInAMonth ;i++){
if(k<31 && k+7<31)
{
for(var j =k ; j<k+7;j++){
if(j<31){
console.log(j);
}
}
k=k+7;
console.log("Next Week");

}
else{
for(var p=k ; p<=31 ;p++){console.log(k);k++;}
}
}
